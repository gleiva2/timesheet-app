{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport moment from 'moment';\nexport default {\n  data: function data() {\n    return {\n      dates: [\"\"],\n      datesPT: [\"\"]\n    };\n  },\n  mounted: function mounted() {\n    console.log(\"mounted\");\n  },\n  computed: {\n    formattedDate: function formattedDate() {\n      if (this.dates[1]) {\n        var formattedString = this.dates[1] ? moment(this.date[1]).format('dddd, MMMM Do YYYY') : '';\n        console.log(formattedString);\n        return this.dates[1] ? moment(this.date[1]).format('dddd, MMMM Do YYYY') : '';\n      } else {\n        return null;\n      }\n    }\n  },\n  methods: {\n    test: function test(date) {\n      console.log(date);\n\n      if (this.dates[1]) {\n        var formattedString = moment(this.date[1]).format('dddd, MMMM Do YYYY');\n        console.log(formattedString);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwCA;AACA;AACAA;AAAA;AACAC,iBADA;AAEAC;AAFA;AAAA,GADA;AAKAC;AACAC;AACA,GAPA;AAQAC;AACAC,iBADA,2BACA;AACA;AACA;AACAF;AACA;AACA,OAJA,MAIA;AACA;AACA;AACA;AATA,GARA;AAmBAG;AACAC,QADA,gBACAC,IADA,EACA;AACAL;;AACA;AACA;AACAA;AACA;AACA;AAPA;AAnBA","names":["data","dates","datesPT","mounted","console","computed","formattedDate","methods","test","date"],"sourceRoot":"src/components","sources":["CleaningHours.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-sheet outlined>\n      <v-toolbar dense dark color=\"primary\">\n        <v-toolbar-title> Cleaning Hours </v-toolbar-title>\n      </v-toolbar>\n\n      <v-date-picker\n        v-model=\"dates\"\n        :landscape=\"false\"\n        multiple\n        elevation=\"5\"\n        no-title\n        @change=\"test\"\n      >\n      </v-date-picker>\n      <p>{{ dates }}</p>\n    </v-sheet>\n    <v-sheet outlined>\n      <v-toolbar dense dark color=\"primary\">\n        <v-toolbar-title> Cleaning Hours PT </v-toolbar-title>\n      </v-toolbar>\n\n      <v-date-picker\n        v-model=\"datesPT\"\n        :landscape=\"false\"\n        multiple\n        elevation=\"5\"\n        no-title\n        :reactive=\"true\"\n        @change=\"test\"\n        @input=\"test\"\n      >\n      </v-date-picker>\n      <p>{{ datesPT }}</p>\n    </v-sheet>\n  </v-container>\n</template>\n\n<script>\nimport moment from 'moment'\nexport default {\n  data: () => ({\n    dates: [\"\"],\n    datesPT: [\"\"],\n  }),\n  mounted: () => {\n    console.log(\"mounted\");\n  },\n  computed: {\n    formattedDate(){\n      if(this.dates[1]){\n        let formattedString = this.dates[1] ? moment(this.date[1]).format('dddd, MMMM Do YYYY') : ''\n        console.log(formattedString)\n        return this.dates[1] ? moment(this.date[1]).format('dddd, MMMM Do YYYY') : ''\n      } else {\n        return null\n      }\n    },\n  },\n  methods: {\n    test(date) {\n      console.log(date);\n      if(this.dates[1]){\n        let formattedString = moment(this.date[1]).format('dddd, MMMM Do YYYY') \n        console.log(formattedString)\n      }\n    },\n  },\n\n};\n</script>"]},"metadata":{},"sourceType":"module"}